{
  "name": "diamant-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator=\"iPhone 11 (13.7)\"",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "app-install": "yarn install && cd ios && pod install && cd ..",
    "android_build_debug": "rm -f android/app/build/outputs/apk/debug/* &&  cd android && ./gradlew app:assembleDebug && cd .. && mkdir -p output && cp -f android/app/build/outputs/apk/debug/*.apk ./output",
    "android_build_release": "cd android && ./gradlew clean && ./gradlew app:assembleRelease && cd .. && mkdir -p output && cp -f android/app/build/outputs/apk/release/*.apk ./output",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "ios11": "react-native run-ios --simulator=\"iPhone 11\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.7",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-firebase/app": "^12.4.0",
    "@react-native-firebase/messaging": "^12.4.0",
    "@react-navigation/bottom-tabs": "^5.11.8",
    "@react-navigation/native": "^6.0.1",
    "@react-navigation/stack": "^6.0.1",
    "@reduxjs/toolkit": "^1.6.1",
    "@types/react-redux": "^7.1.18",
    "@types/styled-components": "^5.1.11",
    "@types/styled-components-react-native": "^5.1.1",
    "gatsby-plugin-resolve-src": "^2.1.0",
    "lottie-ios": "3.2.3",
    "lottie-react-native": "^4.0.3",
    "react": "17.0.1",
    "react-native": "0.64.1",
    "react-native-fast-image": "^8.3.7",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-picker": "^4.0.6",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-modal": "^12.0.3",
    "react-native-reanimated": "^2.2.0",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.5.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-svg": "^12.1.1",
    "react-redux": "^7.2.4",
    "redux-saga": "^1.1.3",
    "styled-components": "^5.3.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^17.0.14",
    "@types/react-native": "^0.64.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/parser": "^4.28.5",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.14.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "17.0.1",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
